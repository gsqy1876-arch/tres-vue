{
    // 使用 IntelliSense 了解相关属性。
    // 悬停以查看现有属性的描述。
    // 欲了解更多信息，请访问: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "启动程序",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${file}"
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Run npm: dev (debug)",
            "runtimeExecutable": "npm.cmd",
            "runtimeArgs": ["run", "dev"],
            "cwd": "${workspaceFolder}",
            "env": {
                "NODE_ENV": "development",
                // "NUXT_LOG_LEVEL": "debug",
                "DEBUG": "nuxt*"
            },
            "console": "integratedTerminal",
            "autoAttachChildProcesses": true,
            "outputCapture": "std"
        }
    ]
}